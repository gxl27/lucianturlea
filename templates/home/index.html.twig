                </div>
            {% endfor %}
            </div>
            <div id='project-details' class="card-holder">
                {% for project in projects %}
                <div class="card">
                    <div class='card-photo'>
                    {% if project.photos %}
                        {% for photo in project.photos %}
                            {% if photo.profile %}
                                <img  src=" {{ vich_uploader_asset(photo)  }} " alt="project-{{ photo.id }}" style='background-color:red'>             
                            {% endif %}
                        {% endfor %}          
                    {% endif %}
                    </div>
                    <div class="card-title">
                        <h2>{{ project.name }}</h2>
                        
                        <h3>{{ project.subtitle }}</h3>
                    </div>
                    <div class="card-content">
                        <div>
                            {{ project.description }}
                        </div>
                        <div>
                            <div>Challenges:</div>
                            <div>
                                {% for challenge in project.challenges %}
                                <li>{{ challenge.name }}</li> 
                                <div style='color:green; margin-bottom:15px'>{{ challenge.solution }}</div>
                                {% endfor %}
                            </div>
                        </div>
                        <div>
                            <div>Overview:</div>
                            <div>
                                {% if project.photos %}
                                    {% for photo in project.photos %}
                                        {% if photo.profile is null %}
                                        <div style='margin-bottom:25px'>
                                            <span>{{ photo.details }}</span>
                                            <img  src=" {{ vich_uploader_asset(photo)  }} " alt="project-{{ photo.id }}" >             
                                            
                                        </div>
                                        {% endif %}
                                    {% endfor %}          
                                {% endif %}
                            </div>    
                        </div>
                    

                    </div>
                    <div class="card-technology">
                        Technologies:
                        {% for tech in project.technology %}
                        <li>{{ tech.name }} - {{ tech.version }}</li>
                        {% endfor %}
                    </div>
                    <div class="card-footer">
                        <div class="btn-secondary">
                            {{ project.public ? "Yes" : "No" }}
                        </div>
                        <div class="btn-secondary">
                             {{ project.production ? "Yes" : "No" }}
                        </div>
                        <div class="btn-primary">
                            {# {{ project.login }} #}
                            login
                        </div>
                        <div class="btn-primary">
                            {# {{ project.link }} #}
                            visit
                        </div>



                    </div>
                </div>
                {% endfor %}
