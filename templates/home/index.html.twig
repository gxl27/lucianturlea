            <div class="subtitle">
                <p>Currently there are {{ projects|length }} projects visible. Some of them require users credential to access them</p>
            
            </div>
            <div id='project-list' class='card-holder'>
            {% for project in projects %}
                <div class="card">
                    <div class="card-photo">
                        <div>
                            {% if project.photos %}
                                {% for photo in project.photos %}
                                    {% if photo.profile %}
                                    <div>
                                        <img src="{{ vich_uploader_asset(photo) }} " alt="project-{{ photo.id }}">             
                                    </div>
                                    {% endif %}
                                {% endfor %}
                            {% endif %}   
                        </div>
                      <div class="card-photo-title">
                        <span>
                            {{ project.name }}
                        </span>
                        <span>
                            {{ project.public ? "Public" : "Private" }}
                        </span>
                      </div>
                    </div>
                    <div class="card-content">
                        <div>
                            <span>{{ project.subtitle }}</span> 
                        </div>
                        <div>
                            <span>tech</span> 
                        </div>
                    </div>
                    <div class="card-bottom">
                        <div class='card-bottom-btn1' title='Jump to the project details'>Details</div>
                        <div class='card-bottom-btn2' itle='Visit the website'>Live visit</div>
                    </div>
                    {# <div class='div-col'>
                        {{ loop.index }}
